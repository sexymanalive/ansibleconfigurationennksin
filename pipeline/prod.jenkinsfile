pipeline{
    agent any
    stages{
        stage('Check Workspace'){
            steps{
                sh """
                whoami
                pwd 
                ls -lrt 

                """
            }
        }
        stage("Check Ansible"){
            steps{
                sh """
                ansible --version
                ansible -i ansible/inventory.ini workers -m ping    
                """
            }
        }
       
       
    }
}